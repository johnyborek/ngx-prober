{
    "name": "ngx-prober",
    "version": "0.0.1",
    "description": "Library for easy and simple Angular testing",
    "author": {
        "name": "Rafal Borek",
        "email": "rr.borek@gmail.com"
    },
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/johnyborek/ngx-prober.git"
    },
    "keywords": [
        "angular",
        "ngx",
        "unit test",
        "unit testing",
        "test",
        "testing",
        "easy",
        "simple",
        "simplified"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/johnyborek/ngx-prober/issues"
    },
    "homepage": "https://github.com/johnyborek/ngx-prober",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "clean": "rimraf ./dist && rimraf ./target",
        "build": "tsc",
        "copy-files": "cpx \"./target/tsc-out/src/lib/**/*.*\" ./dist/lib && cpx \"./target/tsc-out/src/index.*\" ./dist",
        "test": "cd testing-app && ng test --code-coverage && cd ..",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "ng-lint": "ng lint",
        "dist": "npm run clean && npm run lint && npm test && npm run build && npm run copy-files"
    },
    "peerDependencies": {
        "@angular/common": ">=6",
        "@angular/compiler": ">=6",
        "@angular/core": ">=6",
        "@angular/http": ">=6",
        "@angular/platform-browser": ">=6",
        "@angular/router": ">=6",
        "@types/jasmine": ">=2",
        "jasmine-core": ">=2",
        "rxjs": ">=6"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.13.5",
        "@angular/animations": "^7.2.8",
        "@angular/cli": "^7.3.5",
        "@angular/common": "^7.2.8",
        "@angular/compiler": "^7.2.8",
        "@angular/compiler-cli": "^7.2.8",
        "@angular/core": "^7.2.8",
        "@angular/forms": "^7.2.8",
        "@angular/http": "^7.2.8",
        "@angular/language-service": "^7.2.8",
        "@angular/platform-browser": "^7.2.8",
        "@angular/platform-browser-dynamic": "^7.2.8",
        "@angular/router": "^7.2.8",
        "@types/jasmine": "^3.3.9",
        "@types/jasminewd2": "^2.0.6",
        "@types/node": "~10.12.30",
        "core-js": "^2.6.5",
        "cpx": "^1.5.0",
        "jasmine-core": "^3.3.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^4.0.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "^2.0.0",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-jasmine": "^2.0.1",
        "karma-mocha-reporter": "^2.2.5",
        "prettier": "^1.16.4",
        "rimraf": "^2.6.3",
        "run-sequence": "^2.2.1",
        "rxjs": "^6.4.0",
        "ts-node": "^8.0.3",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "~3.1.0",
        "zone.js": "^0.8.29"
    },
    "readme": "README.md"
}
